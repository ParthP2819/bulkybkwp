@model IEnumerable<bulkybkwp.Models.Product>



<div class="row pb-3">
	@foreach(var products in Model)
	{
		<div class="col-lg-3 col-md-6">
			<div class="p-2">
				<div class="col-12 p-1" style="border:2px  black; border-radius:5px;">
					<div class="card" style="width:100%">
						<img src="@products.ImageUrl" class="card-img-top rounded" />
					</div>
				</div>
			</div>
		</div>
	}
</div>